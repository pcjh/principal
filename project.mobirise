          "alias": false,
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "background-color": "@bg",
            "& when (@bgFigures)": {
              "& when (@figure = '1')": {
                ".figure": {
                  "background-color": "@circleColor",
                  "width": "(@circleRadius * 1px)",
                  "height": "(@circleRadius * 1px)",
                  "border-radius": "50%",
                  "top": "(@circlePosY * 1%)",
                  "left": "(@circlePosX * 1%)"
                },
                "& when (@overflow)": {
                  "overflow": "hidden"
                },
                "& when not (@overflow)": {
                  "overflow": "visible"
                }
              },
              "& when (@figure = '2')": {
                ".figure": {
                  "background-color": "@rectColor",
                  "& when not (@directRect) and not (@invertRect)": {
                    "left": "0",
                    "top": "0",
                    "width": "(@sizeRect * 1%)",
                    "height": "100%"
                  },
                  "& when (@directRect) and (@invertRect)": {
                    "right": "0",
                    "bottom": "0",
                    "width": "100%",
                    "height": "(@sizeRect * 1%)"
                  },
                  "& when (@directRect) and not (@invertRect)": {
                    "left": "0",
                    "top": "0",
                    "width": "100%",
                    "height": "(@sizeRect * 1%)"
                  },
                  "& when not (@directRect) and (@invertRect)": {
                    "right": "0",
                    "top": "0",
                    "width": "(@sizeRect * 1%)",
                    "height": "100%"
                  }
                }
              },
              "& when (@figure = '3')": {
                ".figure": {
                  "background": "linear-gradient(@rotateAngle * 1deg, @diagonalColor (@sizeDiagonal * 1%), rgba(0, 0, 0, 0) (@sizeDiagonal * 1%), rgba(0, 0, 0, 0) 100%)",
                  "left": "0",
                  "top": "0",
                  "width": "100%",
                  "height": "100%"
                }
              }
            },
            ".card": {
              "padding": "2px 2px",
              ".image": {
                "height": "(@imgHeight * 1px)",
                "width": "100%",
                ".text-block": {
                  "position": "absolute",
                  "width": "80%",
                  "top": "10%",
                  "left": "10%",
                  "padding": "1rem",
                  "h4,h5": {
                    "margin-bottom": "1rem"
                  }
                },
                ".mbr-section-btn": {
                  "position": "absolute",
                  "bottom": "10%",
                  "left": "10%"
                },
                "amp-img": {
                  "&:after": {
                    "background-color": "@overlayColor",
                    "opacity": "@imgOverlay"
                  }
                }
              },
              "&:hover": {
                "amp-img": {
                  "&:before": {
                    "opacity": "@imgHoverOverlay",
                    "-ms-filter": "\"progid:DXImageTransform.Microsoft.gradient (GradientType=0, startColorstr=#1471da, endColorstr=#1C85FB)\"",
                    "background": "linear-gradient(45deg, @hg1color, @hg2color)"
                  }
                }
              }
            },
